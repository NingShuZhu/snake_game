<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootLayout" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.comp2013cw.snakegame.Controller.EndController">
   <children>
      <Button fx:id="playAgainButton" layoutX="208.0" layoutY="140.0" mnemonicParsing="false" onAction="#playAgain" text="Play Again" textFill="#b776aa">
         <font>
            <Font name="Candara" size="32.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="261.0" layoutY="219.0" mnemonicParsing="false" onAction="#exitGame" text="Exit">
         <font>
            <Font name="Candara" size="26.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
