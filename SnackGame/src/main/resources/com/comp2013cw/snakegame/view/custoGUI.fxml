<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootLayout" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.comp2013cw.snakegame.Controller.CustoController">
   <children>
      <Label layoutX="23.0" layoutY="49.0" prefHeight="49.0" prefWidth="228.0" text="Customize your game:">
         <font>
            <Font name="Candara Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="113.0" prefHeight="38.0" prefWidth="93.0" text="My snake:">
         <font>
            <Font name="Candara" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="snakeCombo" layoutX="137.0" layoutY="118.0" prefHeight="28.0" prefWidth="182.0" />
      <Label layoutX="23.0" layoutY="183.0" prefHeight="38.0" prefWidth="135.0" text="Color scheme:">
         <font>
            <Font name="Candara" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="colorCombo" layoutX="160.0" layoutY="188.0" prefHeight="28.0" prefWidth="159.0" />
      <Label layoutX="23.0" layoutY="253.0" prefHeight="38.0" prefWidth="94.0" text="Music:">
         <font>
            <Font name="Candara" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="musicCombo" layoutX="137.0" layoutY="258.0" prefHeight="28.0" prefWidth="182.0" />
      <Button layoutX="149.0" layoutY="416.0" mnemonicParsing="false" onAction="#clickConfirm" prefHeight="38.0" prefWidth="101.0" text="Confirm">
         <font>
            <Font name="Candara" size="21.0" />
         </font>
      </Button>
      <Button layoutX="149.0" layoutY="474.0" mnemonicParsing="false" onAction="#clickCancel" prefHeight="38.0" prefWidth="101.0" text="Cancel">
         <font>
            <Font name="Candara" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
