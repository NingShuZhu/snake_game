<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootLayout" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.comp2013cw.snakegame.Controller.ConfirmController">
   <children>
      <Label layoutX="34.0" layoutY="51.0" prefHeight="35.0" prefWidth="249.0" text="Are you sure to end the game?">
         <font>
            <Font name="Candara" size="18.0" />
         </font>
      </Label>
      <Button layoutX="62.0" layoutY="112.0" mnemonicParsing="false" onAction="#clickYes" prefHeight="27.0" prefWidth="68.0" text="Yes">
         <font>
            <Font name="Candara" size="18.0" />
         </font>
      </Button>
      <Button layoutX="170.0" layoutY="112.0" mnemonicParsing="false" onAction="#clickNo" prefHeight="27.0" prefWidth="68.0" text="No">
         <font>
            <Font name="Candara" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
